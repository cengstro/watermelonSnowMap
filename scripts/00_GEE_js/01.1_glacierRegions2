var palettes = require('users/gena/packages:palettes');


var version = "V16"
var minArea = 1 // in km2, to show on map (does not affect output)





// display GLIMS glaciers on ui.Map to hand draw features around 
var largeGlaciers = glims.filter(ee.Filter.gt('db_area', minArea)) 
Map.addLayer(largeGlaciers)

// view average summer temps on GrIS to assist in drawing range polygons 
// var tempThresh = 273.15 - 5
// era5=era5
//   .filter(ee.Filter.calendarRange(7,8,'month'))
//   .filter(ee.Filter.calendarRange(2019,2021,'year'))
//   .select('temperature_2m')
//   .mean()
//   .gt(tempThresh)
// Map.addLayer(era5)

// Map.addLayer(wGr) // view the actual algae maps
// Map.addLayer(eGr)

var fc = ee.FeatureCollection([
  // northAmerica
  alaskaPeninsula, alaskaRange, coastNorth, coastSouth,interiorNorth,interiorSouth,cascades, brooks,
  // andes
  andes,
  // europe
  iceland, norwaySouth, norwayNorth, alps,
  // greenland
  northGreenland, southwestGreenland, southeastGreenland,westCentralGreenland, eastCentralGreenland,northwestGreenland,northeastGreenland,
  // greenlandNoSheet,
  // arctic
  ellesmere, baffin, svalbard,russianArctic,
  // centralAsia
  caucasus,altai,highMtnAsia,
  newZealand,
  kamchatka,
  // antarctica
  antarcticaNoSheet //,antarctica
])

// make sure I replaced "name" with "range"
// print(fc.filter(ee.Filter.eq("range",null)))
// print(fc.filter(ee.Filter.neq("name",null)))
print(fc.size())
// print(fc.aggregate_array('region'))
// print(fc.aggregate_array('range'))
Map.addLayer(fc,{},"for export")
Export.table.toAsset(fc, 'glacierRegionsAsset'+version, 'globalSnowAlgae/glacierRegions'+version)

Export.table.toDrive({
  collection: fc,
  description: 'glacierRegionsDrive'+version, 
  folder: "s2_classifier_map",
  fileNamePrefix: 'glacierRegions' +version,
  fileFormat: "KML"
})
  
  
  